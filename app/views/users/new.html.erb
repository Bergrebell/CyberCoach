<h1>Users#new</h1>
<%= form_for(@user, url: users_path, method: :post) do |f| %>

    <p>
    <%= f.label :username %>
    <%= f.text_field :username %>
    </p>

    <p>
    <%= f.label :password %>
    <%= f.text_field :password %>
    </p>

    <p>
    <%= f.label :realname %>
    <%= f.text_field :realname %>
    </p>

    <p>
      <%= f.label :email %>
      <%= f.text_field :email %>
    </p>

    <p>
    <%= f.label :publicvisible %>
    <%= f.text_field :publicvisible %>
    </p>

    <%= f.submit 'Register' %>
<% end %>

